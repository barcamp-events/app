{
  "name": "barcamp-events",
  "version": "1.0.0",
  "description": "The app for barcamp.events",
  "main": "index.js",
  "scripts": {
    "develop:ios": "npx cap open ios",
    "develop:android": "npx cap open android",
    "develop:electron": "npx cap open electron",
    "build": "stencil build",
    "build.prod": "NODE_ENV=production stencil build --next --prod && npx cap copy",
    "start": "stencil build --dev --watch --serve",
    "start.prod": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "rollup-plugin-replace": "^2.2.0",
    "workbox-build": "4.3.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@capacitor/android": "^1.4.0",
    "@capacitor/cli": "^1.4.0",
    "@capacitor/core": "^1.4.0",
    "@capacitor/ios": "^1.4.0",
    "@stellar-design/core": "^1.5.12",
    "@stellar-design/device": "^0.1.2",
    "@stencil/core": "^1.9.0-18",
    "@stencil/router": "^1.0.1",
    "@stencil/state-tunnel": "^1.0.1",
    "async-delay": "^1.0.3",
    "dayjs": "^1.8.19",
    "firebase": "^7.6.1",
    "gravatar": "^1.8.0",
    "konami-code": "^0.2.1",
    "puppeteer": "^2.0.0",
    "rollup-plugin-dotenv": "^0.2.0",
    "slugify": "^1.3.6"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
