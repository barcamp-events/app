{
  "name": "BarCamp Events App",
  "version": 2,
  "env": {
    "IS_NOW": "true"
  },
  "alias": ["beta.barcamp.events"],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
